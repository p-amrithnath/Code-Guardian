{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn",
        "prefer-const": "error",
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off"
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    }
}